  "dependencies": {
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^9.4.0",
    "glob": "^8.0.3",
    "log4js": "^6.6.0",
    "outdent": "^0.8.0",
    "urijs": "^1.19.11"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
